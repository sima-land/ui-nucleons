    "prepare": "husky install",
    "export:colors": "node .template/build.mjs",
    "build:clean": "rm -rf dist",
    "build:main": "babel src --out-dir dist --config-file ./babel.config.build.cjs --source-maps --extensions '.ts,.tsx' --ignore='./**/__test__/**/*' --ignore='./**/__stories__/**/*' --ignore='./**/*.d.ts'",
    "build:types": "tsc -p tsconfig.types.json",
    "build:copy-assets": "node scripts/build-copy-assets.mjs",
    "build:react-fix": "node scripts/build-react-fix.mjs",
    "build": "npm run build:clean && npm run build:main && npm run build:types && npm run build:copy-assets && npm run build:react-fix",
    "test": "BROWSERSLIST_ENV=\"test\" NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" npx jest",
    "coverage": "jest --clearCache && BROWSERSLIST_ENV=\"test\" npx jest --coverage",
    "type-check": "tsc -p . --noEmit",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "eslint --cache src/ --ext .js,.jsx,.ts,.tsx",
    "lint:styles": "stylelint 'src/**/*.{css,scss}'",
    "storybook": "BROWSERSLIST_ENV=\"storybook\" npx storybook dev -p 9009 -c .storybook",
    "storybook:build": "BROWSERSLIST_ENV=\"storybook\" npx storybook build --quiet --loglevel error -o build-storybook",
    "prettier": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "prettier:check": "prettier --check \"./src/**/*.{js,jsx,ts,tsx,css,scss}\""
